{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "ui-client",
            "image": "vaibhav09dev/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "api-server",
            "image": "vaibhav09dev/multi-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "worker",
            "image": "vaibhav09dev/multi-worker",
            "hostname": "worker",
            "essential": false
        },
        {
            "name":"nginx",
            "image": "vaibhav09dev/multi-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings":[
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["ui-client","api-server"]
        }
    ]
}